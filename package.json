{
  "name": "rule-editor",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build --watch ng-rule-editor & sleep 25; ng serve; s=$?; kill %1; exit $s",
    "start-public": "ng build --watch ng-rule-editor & sleep 25; node bin/start-public.js; s=$?; kill %1; exit $s",
    "build:concatenate-gh": "concat -o docs/rule-editor.js dist/rule-editor/runtime.js dist/rule-editor/polyfills.js dist/rule-editor/main.js",
    "build:concatenate": "concat -o dist/rule-editor/rule-editor.js dist/rule-editor/runtime.js dist/rule-editor/polyfills.js dist/rule-editor/main.js",
    "build:gh-pages": "ng build ng-rule-editor --configuration production && ng build rule-editor --configuration production && ng build --configuration production --output-path docs --base-href /rule-editor/ && npm run build:concatenate-gh",
    "build": "ng build ng-rule-editor --configuration production && ng build rule-editor --configuration production && npm run build:concatenate && ng build",
    "test": "ng test --watch=false && ng run demo:cypress-run",
    "test:unit": "ng test --watch=false",
    "lint": "ng lint",
    "test:e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/elements": "^12.2.15",
    "@angular/forms": "~12.2.15",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "@material/tooltip": "^12.0.0",
    "@ungap/custom-elements": "^1.0.0",
    "autocomplete-lhc": "^18.1.2",
    "classlist.js": "^1.1.20150312",
    "fast-copy": "^2.1.1",
    "jquery": "^3.6.0",
    "easy-path-expressions": "^2.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.15",
    "@cypress/schematic": "^1.5.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.17",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "cypress": "8.3.0",
    "cypress-file-upload": "^5.0.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.6",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
