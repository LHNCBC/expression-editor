{
  "name": "rule-editor",
  "version": "3.1.4",
  "scripts": {
    "ng": "ng",
    "start": "ng build --watch ng-rule-editor & sleep 25; ng serve --port 4202; s=$?; kill %1; exit $s",
    "start-public": "ng build --watch ng-rule-editor & sleep 25; node bin/start-public.js; s=$?; kill %1; exit $s",
    "build:concatenate-gh": "concat -o docs/rule-editor.js dist/rule-editor/runtime.js dist/rule-editor/polyfills.js dist/rule-editor/main.js",
    "build:concatenate": "concat -o dist/rule-editor/rule-editor.js dist/rule-editor/runtime.js dist/rule-editor/polyfills.js dist/rule-editor/main.js",
    "build:gh-pages": "ng build ng-rule-editor && ng build rule-editor && ng build --output-path docs --base-href /rule-editor/ && npm run build:concatenate-gh",
    "build": "ng build ng-rule-editor --configuration production && ng build rule-editor --configuration production && npm run build:concatenate && ng build",
    "test": "ng test --watch=false && ng run demo:cypress-run",

    "testw": "ng run demo:cypress-run",
    "test:unit": "ng test --watch=false",
    "lint": "ng lint",
    "test:e2e": "ng e2e --port 4203"
  },
  "dependencies": {
    "@angular/animations": "~15.2.4",
    "@angular/cdk": "~15.2.4",
    "@angular/common": "~15.2.4",
    "@angular/compiler": "~15.2.9",
    "@angular/core": "~15.2.4",
    "@angular/elements": "^15.2.4",
    "@angular/forms": "~15.2.4",
    "@angular/material": "^15.2.3",
    "@angular/platform-browser": "~15.2.4",
    "@angular/platform-browser-dynamic": "~15.2.4",
    "@angular/router": "~15.2.4",
    "@material/tooltip": "^12.0.0",
    "@ungap/custom-elements": "^1.0.0",
    "autocomplete-lhc": "^18.6.2",
    "classlist.js": "^1.1.20150312",
    "fast-copy": "^2.1.1",
    "jquery": "^3.6.0",
    "easy-path-expressions": "^3.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "~15.2.4",
    "@cypress/schematic": "^1.5.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.17",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "cypress": "^9.3.1",
    "cypress-file-upload": "^5.0.8",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.2.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  }
}
