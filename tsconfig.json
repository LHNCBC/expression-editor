  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "questionnaire-rule-editor": [
        "dist/questionnaire-rule-editor/questionnaire-rule-editor",
        "dist/questionnaire-rule-editor"
      ]
    }
  }
}